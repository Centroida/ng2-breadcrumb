{
  "name": "ng2-breadcrumbs",
  "version": "0.0.0-placeholder",
  "description": "Breadcrumb generator for Angular 7",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ts-node -O '{\"module\": \"commonjs\"}' build.ts",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "test:ci": "ng test --watch=false --code-coverage --no-progress",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "ghpages": "ng build --prod --no-progress --baseHref='/ng2-breadcrumbs/'",
    "e2e": "ng e2e",
    "bundlesize": "npx bundlesize",
    "semantic-release": "cd dist && semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Centroida/ng2-breadcrumb"
  },
  "keywords": [
    "Angular 2",
    "Angular 4",
    "Angular 5",
    "Angular 7",
    "ng",
    "Breadcrumbs"
  ],
  "author": "Centroida",
  "license": "MIT",
  "homepage": "https://github.com/Centroida/ng2-breadcrumb",
  "dependencies": {
    "@angular/animations": "7.1.4",
    "@angular/common": "7.1.4",
    "@angular/compiler": "7.1.4",
    "@angular/core": "7.1.4",
    "@angular/forms": "7.1.4",
    "@angular/http": "7.1.4",
    "@angular/platform-browser": "7.1.4",
    "@angular/platform-browser-dynamic": "7.1.4",
    "@angular/router": "7.1.4",
    "core-js": "2.6.1",
    "rxjs": "6.3.3",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.11.4",
    "@angular/cli": "7.1.4",
    "@angular/compiler-cli": "7.1.4",
    "@types/core-js": "0.9.43",
    "@types/fs-extra": "^5.0.4",
    "@types/node": "10.12.18",
    "@types/rimraf": "^2.0.2",
    "codelyzer": "4.5.0",
    "fs-extra": "^7.0.1",
    "ng-packagr": "^4.4.5",
    "rimraf": "^2.6.2",
    "ts-loader": "5.3.1",
    "ts-node": "7.0.1",
    "tsickle": "^0.34.0",
    "tslint": "5.12.0",
    "typescript": "3.1.6"
  }
}
